code="* --- Controllo congruita' Centri di Costo
if i_bRes AND ((g_PERCCR='S' AND .w_FLANAL='S' AND .w_MVFLNOAN='N') OR (g_COMM='S' AND .w_FLGCOM='S')) AND .w_MVTIPRIG $ 'MRF'
   do case
      case EMPTY(.w_MVVOCCEN) AND g_PERCCR='S' AND .w_FLANAL='S'
           i_bRes = .f.
           i_bnoChk = .f.
	         i_cErrorMsg = Ah_MsgFormat("Voce di ricavo non definita")
      case EMPTY(.w_MVCODCOM) AND g_COMM='S' AND .w_FLGCOM='S'
           i_bRes = .f.
           i_bnoChk = .f.
	         i_cErrorMsg = Ah_MsgFormat("Commessa non definita")
      case EMPTY(.w_MVCODCEN) AND g_PERCCR='S' AND .w_FLANAL='S' AND .w_MVFLNOAN='N'
	         i_bRes = .f.
	         i_bnoChk = .f.		
	         i_cErrorMsg = Ah_MsgFormat("Centro di ricavo non definito")
      endcase
endif

* --- Controllo codice responsabile
if i_bRes AND IsAlt() AND EMPTY(.w_MVCODRES) AND .w_MVTIPRIG $ 'MRF'
    i_bRes = .f.
    i_bnoChk = .f.
	  i_cErrorMsg = Ah_MsgFormat("Codice responsabile non definito")
        endif

* --- Controllo codice pratica
if i_bRes AND IsAlt() AND .w_FLPRAT='S' AND EMPTY(.w_MVCODCOM) AND .w_MVTIPRIG $ 'MRF' AND g_PRAT='S'
    i_bRes = .f.
    i_bnoChk = .f.
	  i_cErrorMsg = Ah_MsgFormat("Codice pratica non definito")
endif

* --- Controllo congruita' Commessa
if i_bRes AND g_COMM='S' AND .w_FLGCOM='S' AND .w_MVTIPRIG $ 'MRF' AND .w_FLCOMM<>'N'
   if EMPTY(.w_MVCODATT) AND NOT EMPTY(.w_MVCODCOM)
      i_bRes = .f.
      i_bnoChk = .f.
      i_cErrorMsg = Ah_MsgFormat("Attività di commessa non definita")
   endif
endif

* --- Controllo data prestazione
if i_bRes AND IsAlt() AND .w_MVTIPRIG $ 'MF'
   if EMPTY(.w_MVDATOAI)
      i_bRes = .f.
      i_bnoChk = .f.
      i_cErrorMsg = Ah_MsgFormat("Data prestazione non definita")
   endif
endif

