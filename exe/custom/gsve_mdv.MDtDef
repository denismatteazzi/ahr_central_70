*--- Calcolo spese Trasporto/Imballo
Proc Calcola_Spese_ImbTra(pTipo)
   Local l_OldValue
   If pTipo='I'
     This.w_CHKSPEIMB = .T.
     l_OldValue = This.w_FLSPTR
     This.w_FLSPTR='N'
     GSAR_BST(This, .T.)
     This.w_CHKSPEIMB = .F.
     This.w_FLSPTR = l_OldValue
   Else
     This.w_CHKSPETRA = .T.
     l_OldValue = This.w_FLSPIM
     This.w_FLSPIM='N'
     GSAR_BST(This, .T.)
     This.w_CHKSPETRA = .F.
     This.w_FLSPIM = l_OldValue
   Endif
Endproc

Proc EcpLoad()
   DoDefault()
   if !empty(this.w_ZOOMFILTER)
      SetValueLinked("MT", this, "w_MVTIPDOC", this.pDocFiltr)
   endif   
   if !empty(this.w_MVTIPDOC) and this.w_MVNUMDOC<>0
   Local controllo
    do case
     case !(this.w_MVCLADOC='RF' OR ( !INLIST(this.w_TDRICNOM,'N',' ') AND (g_OFFE='S' OR g_AGEN='S') AND this.cFUNCTION = "Load" ))
      controllo = this.GETCTRL('w_MVCODCON')
     case!(this.w_MVCLADOC='RF' OR INLIST(this.w_TDRICNOM,'N',' ') OR (g_OFFE<>'S' AND g_AGEN<>'S') OR this.cFUNCTION <> "Load")
      controllo = this.GETCTRL('w_NOCODICE')
    Endcase
     if vartype(controllo)='O'
      controllo.SetFocus()
    Endif
   Endif
Endproc

* --- Ridefinizione ecpSave per uscire subito dalla gestione in fase di caricamento
Procedure ecpSave()
    DoDefault()
    If this.w_Loc_Error=' ' AND IsAlt() AND this.cFunction="Load"
      this.cFunction="Query"
      this.ecpQuit()
    Endif
Endproc
