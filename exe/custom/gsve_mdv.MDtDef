if i_bRes AND .w_GIACON='N'
	         i_bRes = .f.
	         i_bnoChk = .f.		
	         i_cErrorMsg = Ah_MsgFormat("Riga a disponibilità negativa")
Endif

IF i_bRes AND (((g_PERLOT='S' AND .w_FLLOTT $ 'SC') OR (g_PERUBI='S'AND (.w_FLUBIC='S' OR .w_F2UBIC='S')))AND(((.w_MVFLCASC='-'AND .w_FLRRIF <>'+') OR .w_MVFLRISE $'+-' OR .w_MVFLCASC='+');
   OR ((.w_MVF2CASC='-'AND .w_FLRRIF <>'+') OR .w_MVF2RISE $'+-' OR .w_MVF2CASC='+') ) AND ((.cFunction='Load') OR (.cFunction='Edit'))AND .w_MVFLPROV <>'S' AND .w_FLDISP$ 'SC' AND .w_DISLOT ='S')
   i_bRes = CHKMOVLO(.w_MVCODLOT,.w_MVCODUBI,.w_MVCODUB2,.w_MVCODMAG,.w_MVCODMAT,.w_MVFLLOTT,.w_MVF2LOTT,;
   .w_MVQTAUM1-.w_OLDQTA,.w_MVCODART,.w_FLLOTT,.w_FLUBIC,.w_F2UBIC,.w_FLSTAT,.w_MVSERIAL,'D',.w_MVSERRIF, NOT.w_TESLOT)
   i_bnoChk = .t.	
   i_cErrorMsg = ""
ENDIF
IF IsAlt() and not i_bRes
    * solo se ALterego e i_bRes vale false
